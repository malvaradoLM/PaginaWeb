<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="spCatVehiculoDS" xmlns="" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" xmlns:msprop="urn:schemas-microsoft-com:xml-msprop">
  <xs:element name="spCatVehiculoDS" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element name="spCatVehiculo" msprop:Schema="spCatVehiculo">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="VehiculoID" type="xs:int" />
              <xs:element name="DigitoSeguridad" type="xs:int" minOccurs="0" />
              <xs:element name="Nombre">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="30" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="Identificacion" minOccurs="0">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="30" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="Departamento" minOccurs="0">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="20" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="CuentaContable" minOccurs="0">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="20" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="CentroCostos" minOccurs="0">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="15" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="NumeroEconomico" minOccurs="0">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="15" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="NIP">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="8" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="Estatus">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="1" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="CargasMaximas" type="xs:int" />
              <xs:element name="Odometro" type="xs:boolean" />
              <xs:element name="Firma" type="xs:boolean" />
              <xs:element name="Placas" type="xs:boolean" />
              <xs:element name="FechaAlta" type="xs:dateTime" />
              <xs:element name="FechaExpira" type="xs:dateTime" />
              <xs:element name="FechaBaja" type="xs:dateTime" minOccurs="0" />
              <xs:element name="FechaCancelacion" type="xs:dateTime" minOccurs="0" />
              <xs:element name="FechaBloqueada" type="xs:dateTime" minOccurs="0" />
              <xs:element name="FechaInactiva" type="xs:dateTime" minOccurs="0" />
              <xs:element name="Mensaje" minOccurs="0">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="50" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="LimiteLTTurno" type="xs:double" />
              <xs:element name="LimiteLTDia" type="xs:double" />
              <xs:element name="LimiteLTSemana" type="xs:double" />
              <xs:element name="LimiteLTMes" type="xs:double" />
              <xs:element name="LimiteMNTurno" type="xs:double" />
              <xs:element name="LimiteMNDia" type="xs:double" />
              <xs:element name="LimiteMNSemana" type="xs:double" />
              <xs:element name="LimiteMNMes" type="xs:double" />
              <xs:element name="ActualLTTurno" type="xs:double" />
              <xs:element name="ActualLTDia" type="xs:double" />
              <xs:element name="ActualLTSemana" type="xs:double" />
              <xs:element name="ActualLTMes" type="xs:double" />
              <xs:element name="ActualMNTurno" type="xs:double" />
              <xs:element name="ActualMNDia" type="xs:double" />
              <xs:element name="ActualMNSemana" type="xs:double" />
              <xs:element name="ActualMNMes" type="xs:double" />
              <xs:element name="ActualCargas" type="xs:int" />
              <xs:element name="Clasificacion" type="xs:double" />
              <xs:element name="Generada" type="xs:boolean" />
              <xs:element name="Lunes">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="24" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="Martes">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="24" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="Miercoles">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="24" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="Jueves">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="24" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="Viernes">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="24" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="Sabado">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="24" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="Domingo">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="24" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="ClienteID" type="xs:int" />
              <xs:element name="GasolineroID" type="xs:int" />
              <xs:element name="ProductoAutorizado">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="50" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="IngresarDirecto" type="xs:boolean" minOccurs="0" />
              <xs:element name="Estacion" minOccurs="0">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="150" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="AnteriorLTDia" type="xs:double" minOccurs="0" />
              <xs:element name="AnteriorLTSemana" type="xs:double" minOccurs="0" />
              <xs:element name="AnteriorLTMes" type="xs:double" minOccurs="0" />
              <xs:element name="AnteriorMNDia" type="xs:double" minOccurs="0" />
              <xs:element name="AnteriorMNSemana" type="xs:double" minOccurs="0" />
              <xs:element name="AnteriorMNMes" type="xs:double" minOccurs="0" />
              <xs:element name="PlacasNumero" minOccurs="0">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="10" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="Fotos" minOccurs="0">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="150" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:complexType>
  </xs:element>
</xs:schema>